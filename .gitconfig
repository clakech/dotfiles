[http]
[https]
[user]
	name = Cyril Lakech
	email = cyril.lakech@axa.fr
[alias]
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	co = checkout
[core]
	autocrlf = input
	editor = code --wait
	excludesfile = /Users/cyril.lakech/.gitignore_global
	pager = less -r
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[branch]
	# autosetuprebase = always
	# autosetupmerge = always
[push]
	default = simple
[commit]
	template = /Users/cyril.lakech/.stCommitMsg
